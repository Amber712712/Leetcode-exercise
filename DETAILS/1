a=[[-5,-2],[-1,2],[-3,2]]
l=[]
for i in a:
    for j in range(i[0],i[1]+1): #######注意
        l.append(j)
####统计出现频率
dic={}
for i in l:
    dic[i]=dic.get(i,0)+1

####### 以两个元素排序
sorted(dic.items(),key=lambda x:(-x[1],x[0]))


