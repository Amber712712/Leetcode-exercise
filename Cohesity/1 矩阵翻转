#顺时针90
def func(l):
    n=len(l)
    ##水平翻转
    for r in range(n//2):
        for c in range(n):
            l[r][c],l[n-i-r][c]=
    ##主对角线     
    for i in range(n):
        for j in range(i):
            l[i][j],l[j][i]
            
            
        
        
##副对角线
for i in range(n):
    for j in range(n-i):
        l[i][j],l[n-i-1][n-j-1]
        
        
           
           
           
